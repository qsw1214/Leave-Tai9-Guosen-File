{
  "name": "hr-system_modules",
  "version": "1.7.1-beta.0",
  "description": "人力资源系统公共组件：modules, utils",
  "author": "Alan Ouyang <alanoy11@gmail.com>",
  "private": true,
  "scripts": {
    "init": "node build/init.js",
    "static": "node build/static-server.js",
    "dev": "node build/dev-server.js",
    "build": "cross-env NODE_ENV=production node build/build.js",
    "test": "cross-env NODE_ENV=testing node build/build.js",
    "dev-be": "cross-env LOCAL=false NODE_ENV=testing node build/build.js",
    "local": "cross-env LOCAL=true NODE_ENV=testing node build/build.js",
    "lint": "eslint --ext .js,.vue src && stylelint src/**/*.vue src**/*.scss --syntax scss",
    "mock": "mock-api serve src/mock -p 8080",
    "theme-default": "node_modules/.bin/et -m -c src/assets/theme-default/theme-default.scss -o dist/theme/default",
    "bump": "bump --prompt --grep src/utils/index.js",
    "release-dev": "npm run init && npm run theme-default && npm run dev-be",
    "release-test": "bump --prompt --commit --push --grep src/utils/index.js && npm run init && npm run theme-default && npm run test",
    "release": "bump --prompt --commit --tag --push --grep src/utils/index.js && npm run init && npm run theme-default && npm run build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "element-ui": "^2.3.7",
    "little-loader": "^0.2.0",
    "nprogress": "^0.2.0",
    "vue": "^2.5.16",
    "vue-cropper": "^0.2.9",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.4.1",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "element-theme": "^2.0.1",
    "element-theme-chalk": "2.3.7",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-vue": "^4.5.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^6.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "mock-api": "^0.1.5",
    "node-sass": "^4.9.0",
    "opn": "^5.3.0",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "ora": "^2.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "semver": "^5.5.0",
    "shelljs": "^0.8.2",
    "stylelint": "^9.2.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.1.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.2",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.11.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.2"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:vue",
    "onLanguage:scss",
    "onLanguage:css",
    "onLanguage:html"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}